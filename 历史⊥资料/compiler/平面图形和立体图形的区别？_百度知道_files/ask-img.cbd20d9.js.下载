define("question-new:widget/ask/pc-exp/ask-img",function(i){var e=i("common:widget/lib/jquery/jquery"),n=i("question-new:widget/read-opt/img-preview/preview"),t=i("question-new:widget/ask/ask").transObj;e(".q-img-wp").each(function(i,n){var o=e(n),r=o.find(".q-img-ul"),l=o.find(".q-img-li"),a=o.find(".q-img-con"),p=r.width(),s=a.width();s>=p&&(o.find(".q-img-prev-wp").remove(),o.find(".q-img-next-wp").remove()),o.find(".q-img-next").on("click",function(){var i=r.position().left;r.animate(p+i>=2*s?{left:"-"+(s-i)+"px"}:{left:"-"+(p-s)+"px"})}),o.find(".q-img-prev").on("click",function(){var i=r.position().left;r.animate(-s>=i?{left:i+s+"px"}:{left:"0px"})}),l.on("click",function(){var i=e(this).find(".play-icon-tiny");if(i.length)return void t.videoBox.play();var n=e(this).find(".q-img-item").clone(),o=e('<div class="q-img-fullscreen-wraper"></div>');o.append(n),e("body").append(o),e(this).closest(".replyask-box").size()||e("body").addClass("q-img-body-noscroll"),n.css({marginLeft:-n.width()/2+"px",marginTop:-n.height()/2+"px"}),o.click(function(i){i.stopPropagation(),e("body").removeClass("q-img-body-noscroll"),e(this).remove()})}),e.trim(o.prev().html()).replace(/<br>/g,"")||o.prev("pre").remove()}),"view"===F.context("pageType")&&(e(".replyask-box .ikqb_img_alink").click=null,new n({$el:".replyask-box .ikqb_img_alink",imgAttr:"href"}))});